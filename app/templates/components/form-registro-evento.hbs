<div class="container">
  <div class="row">
    <div class="col s12">
      <label for="nombre">Nombre</label>
        {{input id="nombre" type="text" value=evento.nombre}}
      </div>
  </div>
  <div class="row">
    <div class="col s6">
      <label for="fecha">Fecha</label>
      {{input id="fecha" type="date" value=evento.fecha}}
    </div>
    <div class="col s6">
      <label for="hr">Hora de Inicio</label>
      {{input id="hr" type="date" value=evento.horaInicio}}
    </div>
  </div>

</div>
